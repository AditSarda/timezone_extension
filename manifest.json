{
    "manifest_version": 2,
    "name": "Timezone",
    "version": "2.0.1",
    "description": "This extension displays the correct time zone",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Timezone"
    },
    "permissions": ["activeTab"],
    "background": {
        "scripts": ["main.js"]
    }

}